{
  "name": "@demonicattack/prettier",
  "private": true,
  "type": "module",
  "exports": "./dist/index.js",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "pnpm build:clean && tsup",
    "build:clean": "rimraf dist",
    "check-types": "tsc --noEmit",
    "clean": "rm -rf .turbo node_modules",
    "format:check": "prettier --check . --cache --cache-location node_modules/.cache/.prettiercache --config ./prettier.config.js --log-level=warn",
    "format:debug": "prettier --check . --cache --cache-location node_modules/.cache/.prettiercache --config ./prettier.config.js --log-level=debug",
    "format:fix": "prettier --write . --cache --cache-location node_modules/.cache/.prettiercache --config ./prettier.config.js --log-level=warn",
    "test": "vitest"
  },
  "dependencies": {
    "local-pkg": "^0.5.0",
    "prettier": "^3.3.3",
    "prettier-plugin-multiline-arrays": "^3.0.6",
    "prettier-plugin-packagejson": "^2.5.3",
    "prettier-plugin-prisma": "^5.0.0",
    "prettier-plugin-sort-json": "^4.0.0",
    "prettier-plugin-tailwindcss": "^0.6.8"
  },
  "devDependencies": {
    "@demonicattack/typescript": "workspace:*",
    "rimraf": "^6.0.1",
    "tsup": "^8.3.0",
    "typescript": "^5.6.3",
    "vitest": "^2.1.3"
  },
  "packageManager": "pnpm@9.12.2"
}
