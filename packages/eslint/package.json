{
  "name": "@demonicattack/prettier",
  "private": true,
  "type": "module",
  "exports": "./dist/index.js",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "pnpm build:clean && tsup",
    "build:clean": "rimraf dist",
    "build:inspector": "pnpm build && pnpm dlx @eslint/config-inspector build",
    "check-types": "tsc --noEmit",
    "clean": "rm -rf .turbo node_modules",
    "dev": "pnpm typegen && pnpm dlx @eslint/config-inspector --config eslint.config.ts",
    "lint": "eslint",
    "lint:fix": "eslint --fix"
  },
  "dependencies": {
    "confusing-browser-globals": "^1.0.11",
    "eslint-config-eslint": "^11.0.0",
    "local-pkg": "^0.5.0"
  },
  "devDependencies": {
    "@demonicattack/typescript": "workspace:*",
    "@eslint/js": "^9.13.0",
    "@types/node": "^22.7.7",
    "eslint": "^9.13.0",
    "globals": "^15.11.0",
    "rimraf": "^6.0.1",
    "tsup": "^8.3.0",
    "typescript": "^5.6.3",
    "typescript-eslint": "^8.10.0"
  },
  "packageManager": "pnpm@9.12.2"
}
